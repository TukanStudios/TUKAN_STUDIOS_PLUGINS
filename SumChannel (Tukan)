gfx_r=0;gfx_b=1;gfx_g=0.7;
gfx_rect(reduct_mini,gfx_h-(gfx_h/8),gfx_w,gfx_h);


      mnmetergfx(0, 0, 0, 0, 1,  glbl_desk - 1);








