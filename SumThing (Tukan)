      gcal.collision()  ? gcal.potWheel():
     mouse_wheel=0;
    );


gamount.dragging  ? (gamount.potDrag();Changer = 1);
gnoise.dragging ? (gnoise.potDrag();Changer = 1);
glo.dragging ? (glo.potDrag();Changer = 1);
ghi.dragging ? (ghi.potDrag();Changer = 1);
