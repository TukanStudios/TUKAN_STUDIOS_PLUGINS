                    gspeed.potVal(gspeed.val);
                    gspeed.valOld=gspeed;
                  );
              
               gcal!=slider30?
                   (
                     gcal=slider30;
                     gcal.val=gcal;
                     gcal.potVal(gcal.val);
                     gcal.valOld=gcal;
                   );
                   
              stattslider();
initial = 0;
);


/********************************* Mouse Control ******************************/
mouseClick == 1 ? 
    (
