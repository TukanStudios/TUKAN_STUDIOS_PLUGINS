            gpeakg=slider8;
            gpeakg.val=gpeakg;
            gpeakg.potVal(gpeakg.val);
            gpeakg.valOld=gpeakg;

            gpeakq=slider9;
            gpeakq.val=gpeakq;
            gpeakq.potVal(gpeakq.val);
            gpeakq.valOld=gpeakq;

            goutput=slider10;
            goutput.val=goutput;
