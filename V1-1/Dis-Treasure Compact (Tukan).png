function tanh(x)
(
x = exp(2*x);
(x - 1) / (x + 1);
);

function cosh(x)
(
(exp(x) + exp(-x))/2;
);

// First antiderivative of tanh(x)
function tanh0(x)
(
log(cosh(x));
);

